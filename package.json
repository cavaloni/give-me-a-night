{
  "name": "thinkful-react-starter",
  "version": "0.0.1",
  "description": "A React template",
  "main": "server.js",
  "engines": {
    "node": "7.3.0",
    "npm": "4.0.5"
  },
  "scripts": {
    "start": "node server.js",
    "prod": "run-p start build-prod",
    "heroku-postbuild": "npm run build-prod",
    "dev": "run-p watch serve build",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "run-s _build:*",
    "build-prod": "run-s build:css build:js-prod build:css build:assets",
    "clean": "rimraf build",
    "serve": "http-server -c-1 build",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "npm run-s clean:js mkdir:js _build:js",
    "build:js-prod": "webpack -p --progress --colors --config webpack.prod.config.js",
    "_build:js": "webpack -p",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch --progress",
    "clean:css": "npm run rimraf build/css",
    "mkdir:css": "npm run-s mkdir build/css",
    "build:css": "npm run-s clean:css mkdir:css _build:css",
    "_build:css": "npm run cpr css build/css",
    "watch:css": "npm run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "npm run-s mkdir build/assets",
    "clean:assets": "npm run rimraf build/assets",
    "build:assets": "npm run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "npm run cpr assets build/assets",
    "watch:assets": "npm run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "author": "Joe Turner <joe@oampo.co.uk>",
  "license": "UNLICENSED",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.14.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "mocha": "^3.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack-bundle-analyzer": "^2.2.1"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-css-modules": "^2.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "express": "^4.14.1",
    "fetch-jsonp": "^1.0.5",
    "file-loader": "^0.10.0",
    "geocoder": "^0.2.2",
    "geocoder-geojson": "^1.10.5",
    "google-locations": "^0.2.0",
    "google-places-browser": "^1.0.0",
    "google-places-web": "0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "immutability-helper": "^2.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "lodash.isequal": "^4.5.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "net": "^1.0.2",
    "nonce": "^1.0.4",
    "npm-run-all": "^3.0.0",
    "oauth-signature": "^1.3.1",
    "object-path": "^0.11.3",
    "object-path-immutable": "^0.5.1",
    "querystring": "^0.2.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.3.1",
    "react-google-maps-loader": "^2.0.2",
    "react-google-places-suggest": "^2.0.1",
    "react-motion": "^0.4.7",
    "react-overlays": "^0.6.10",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-router-transition": "0.0.6",
    "react-selectize": "^2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.1.0",
    "style-loader": "^0.13.1",
    "uglify-js": "github:mishoo/UglifyJS2#harmony",
    "webpack": "^2.2.1",
    "whatwg-fetch": "^2.0.2"
  }
}
